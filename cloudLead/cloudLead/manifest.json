{
    "manifest_version" : 3,
    "name" : "CloudLead",
    "description" : "Easy to use",
    "version" : "1.0.0",
    "icons" : {
        "128" : "small.png"   
    },
    "permissions": [
        "tabs",
        "https://*.linkedin.com/*",
        "storage"
      ],
    "action" : {
        "default_icon" : "small.png",
        "default_popup" : "popup.html"
    },
    "background" : {
        "service_worker" : "background.js"    
    },
    "host_permissions": [
        "https://*.linkedin.com/*"
      ]


}